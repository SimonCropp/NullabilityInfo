<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net5;net6;net48</TargetFrameworks>
    <NoWarn>$(NoWarn);CS0649;CS8618;CS0105</NoWarn>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(OS)' != 'Windows_NT' ">
    <TargetFrameworks>net5;net6</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\Nullability.Source\NullabilityInfoExtensions.cs.pp" Link="NullabilityInfoExtensions.cs" />
    <Compile Include="..\Nullability.Source\NullabilityInfo.cs.pp" Link="NullabilityInfo.cs" Condition="$(TargetFramework) != 'net6'" />
    <Compile Include="..\Nullability.Source\NullabilityInfoContext.cs.pp" Link="NullabilityInfoContext.cs" Condition="$(TargetFramework) != 'net6'" />
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
    <PackageReference Include="Verify.Xunit" Version="16.1.1" />
    <PackageReference Include="Xunit" Version="2.4.1" />
    <PackageReference Include="xunit.runner.visualstudio" Version="2.4.3" PrivateAssets="all" />
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="17.0.0" />
    <PackageReference Include="MarkdownSnippets.MsBuild" Version="24.3.0" PrivateAssets="all" />
    <PackageReference Include="ProjectDefaults" Version="1.0.68" PrivateAssets="all" />
  </ItemGroup>
</Project>